      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "[('Django', 29.25),\n",
              " ('Java', 69),\n",
              " ('Python', 105.98),\n",
              " ('Julia', 264),\n",
